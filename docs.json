{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#248168",
    "light": "#248168",
    "dark": "#248168"
  },
  "favicon": "/images/ULCC-190d8a81.png",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "icon": "link-horizontal",
          "href": "https://frontier-va.com"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/images/1.png",
    "dark": "/images/2.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:help@frontier-va.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/NqMjxxKwM6"
    }
  },
  "background": {
    "decoration": "windows"
  },
  "fonts": {
    "family": "Geist"
  },
  "banner": {
    "dismissible": false,
    "content": "Frontier VA"
  }
}
